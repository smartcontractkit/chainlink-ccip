          "isMut": false,
          "isSigner": false
        },
        {
          "name": "tokenPoolsSigner",
          "isMut": false,
          "isSigner": false
        }
      ],
      "args": [
        {
          "name": "rawExecutionReport",
          "type": "bytes"
        },
        {
          "name": "reportContextByteWords",
          "type": {
