# Job Distributor (JD) connection settings.
# Copy this file to .env and fill in your credentials:
#   cp env.sample .env
#
# These variables are loaded automatically when the -nops flag is used.
# Variables already exported in your shell take precedence over values here.

JD_GRPC_URL = "jd.example.com:443"
JD_TLS = true
JD_COGNITO_CLIENT_ID = "your-cognito-client-id"
JD_COGNITO_CLIENT_SECRET = "your-cognito-client-secret"
JD_USERNAME = "your-username"
JD_PASSWORD = "your-password"
JD_AWS_REGION = "us-west-2"
