pullSecrets:
  regcred-prod-ecr:
    registry: 804282218731.dkr.ecr.us-west-2.amazonaws.com
    secret: regcred-prod
deployments:
  {{- range .RMNNodes }}
  rmn-{{ .NodeID }}:
    helm:
      values:
        imagePullSecrets:
          - name: regcred-prod
        rageproxy:
          image:
            tag: master-arm64${RAGE_PROXY_IMAGE_SHA}
        rmn:
          image:
            tag: master-arm64-${RMN_IMAGE_SHA}
        enable_local: false
        persistence:
          enabled: false
  {{- end }}
